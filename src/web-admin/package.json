{
  "name": "@teamb/web-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:client\" \"pnpm dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/index.ts",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json --listFiles --listEmittedFiles",
    "preview": "vite preview",
    "start": "node --import tsx dist/server/web-admin/src/server/index.js",
    "clean": "rm -rf dist .next",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@fastify/cookie": "^10.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.4.0",
    "@fastify/static": "^9.0.0",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-router": "^1.58.3",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "chartjs": "^0.3.24",
    "d3-cloud": "^1.2.8",
    "drizzle-orm": "^0.44.7",
    "fastify": "^5.2.0",
    "file-saver": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.563.0",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tanstack/router-devtools": "^1.58.3",
    "@tanstack/router-vite-plugin": "^1.58.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.53.1",
    "vite": "^5.4.8"
  }
}
