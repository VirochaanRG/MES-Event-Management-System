{
  "expo": {
    "name": "Team D Mobile",
    "slug": "teamd-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.largeevent.teamd",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera access is required to scan event tickets."
      }
    },
    "android": {
      "package": "com.largeevent.teamd",
      "permissions": ["CAMERA"],
      "usesCleartextTraffic": true
    },
    "plugins": [
      "expo-camera"
    ]
  }
}
